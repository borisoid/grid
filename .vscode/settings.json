{
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "mypy-type-checker.importStrategy": "fromEnvironment",
    "files.exclude": {
        "**/__pycache__": true,
        "**/.mypy_cache": true
    },
    "cSpell.words": [
        "KEYDOWN",
        "kiwisolver",
        "MOUSEBUTTONDOWN",
        "MOUSEBUTTONUP",
        "pygame"
    ],
}
